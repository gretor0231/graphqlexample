type Query {
  books: [Book!]!
  students: [Student!]!
}

type Mutation {
  createBook(title: String!, author: String!, published: String!): Book!
  createStudent(
    firstname: String!
    lastname: String!
    age: Int!
  ): Student!
}

type Book {
  id: ID!
  title: String!
  author: String!
  published: String!
}

type Student {
  id: ID!
  firstname: String!
  lastname: String!
  age: Int!
}
